<div class="module-engagement-rings module-bands womens">
 
  <div class="collection-header waypoint-active" data-waypoint="sticky">
    <div class="header-placeholder"></div>
    <div class="header-sticky">
       {% for block in section.blocks %}
      <a class="link-nav" href="#collection-round{{ forloop.index }}">{{ block.settings.Roundtetx }}</a>
       {% endfor %}
    </div>
  </div>

  <div class="collection">
    <div class="product-groups">
      {% for block in section.blocks %}
      <div id="collection-round{{ forloop.index }}" class="product-group round waypoint-active" data-waypoint="scrollLink">
        <h4>{{ block.settings.Roundtetx }}</h4>
        <h5>{{ block.settings.Starting }}</h5>
        <div class="grid">
          <a class="product yellow-gold with-link-primary" href="{{ block.settings.alink1 }}">
            <img src="{{ block.settings.proimg1|img_url: 'master' }}">
            <span class="link-primary">{{ block.settings.Gold1 }}</span>
          </a>
          <a class="product yellow-gold with-link-primary" href="{{ block.settings.alink2 }}">
            <img src="{{ block.settings.proimg2 |img_url: 'master'}}">
            <span class="link-primary">{{ block.settings.Gold2 }}</span>
          </a>
         <a class="product yellow-gold with-link-primary" href="{{ block.settings.alink3 }}">
            <img src="{{ block.settings.proimg3|img_url: 'master' }}">
            <span class="link-primary">{{ block.settings.Gold3 }}</span>
          </a>
          <a class="product yellow-gold with-link-primary" href="{{ block.settings.alink4 }}">
            <img src="{{ block.settings.proimg4 |img_url: 'master'}}">
            <span class="link-primary">{{ block.settings.Gold4 }}</span>
          </a>
        </div>
        
      </div>
  {% endfor %}
    </div>
  </div>
</div>
