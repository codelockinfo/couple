    "b80a629e-83e8-4851-9448-8bbe1b2d3e3b": {
      "type": "hero_section",
      "settings": {
        "heroimg": "shopify:\/\/shop_images\/inbloom-d-plp-pave-atf-2x.jpg",
        "subhading": "OUR RINGS",
        "hading": "The Pavé",
        "textp": "A refined expression of joy and romance.",
